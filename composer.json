{
    "name": "clvarley/php-skeleton",
    "description": "Scaffold repo for spinning up new PHP projects quickly",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Clvarley\\PhpSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Clvarley\\PhpSkeleton\\Test\\": "test/"
        }
    },
    "require": {
        "php": ">= 7.4"
    },
    "require-dev": {
        "vimeo/psalm": "^5.11",
        "phpunit/phpunit": "^9.6.8"
    }
}
